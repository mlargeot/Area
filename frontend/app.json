{
  "expo": {
    "name": "ar3m",
    "slug": "ar3m",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
      "scheme": "comma-mobile",
      "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
	"bundler": "metro",
	"favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
